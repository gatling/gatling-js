{
  "name": "gatling-ts-demo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "target/bundle",
  "dependencies": {
    "@gatling.io/core": "../tmp/core/package",
    "@gatling.io/grpc": "../tmp/grpc/package",
    "@gatling.io/http": "../tmp/http/package",
    "@gatling.io/mqtt": "../tmp/mqtt/package",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@gatling.io/cli": "../tmp/cli/package",
    "@gatling.io/jvm-types": "../tmp/jvm-types/package",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "typescript": "5.9.2"
  },
  "scripts": {
    "clean": "rimraf target",
    "format": "prettier --write '**/*.ts'",
    "setup": "gatling install",
    "check": "tsc --noEmit",
    "build": "tsc --noEmit && gatling build --typescript",
    "start": "gatling run",
    "recorder": "gatling recorder --typescript"
  }
}
