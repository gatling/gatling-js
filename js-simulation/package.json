{
  "name": "gatling-js-demo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "target/bundle",
  "dependencies": {
    "@gatling.io/core": "0.0.0",
    "@gatling.io/http": "0.0.0"
  },
  "devDependencies": {
    "@gatling.io/cli": "0.0.0",
    "prettier": "3.4.1",
