{
  "name": "gatling-js",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "target/bundle",
  "dependencies": {},
  "devDependencies": {
    "rimraf": "5.0.5",
    "prettier": "3.2.5",
    "typescript": "5.4.2"
  },
  "scripts": {
    "clean": "rimraf target results tmp",
    "format": "prettier --write '**/*.ts'",
    "setup": "gatling-js-cli install",
    "build": "gatling-js-cli build --typescript",
    "start": "tsc --noEmit && gatling-js-cli run --typescript"
  }
}
